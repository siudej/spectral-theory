Cell[CellGroupData[{

Cell["Algorithm for polynomial inequalities", "Section",
 CellChangeTimes->{{3.548980339841576*^9, 3.5489803621724443`*^9}}],

Cell["\<\
PolyNeg[P,{x,y},{dx,dy}]: show that polynomial P with variables x, y is \
nonpositive on rectangle (0, 0) (dx, dy). True means it is, False means \
algorithm failed. See http://www.ams.org/mathscinet-getitem?mr=MR2779073 for \
detail.

We can use this on large polynomials in all other cases.\
\>", "Text",
 CellChangeTimes->{{3.5489804760155354`*^9, 3.5489806103938837`*^9}, {
  3.5490248846688337`*^9, 3.549024912106557*^9}, {3.54903673509548*^9, 
  3.549036739102003*^9}, {3.549039338186799*^9, 3.549039341840995*^9}, {
  3.549045851844508*^9, 3.549045891470367*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"CumFun", "[", 
    RowBox[{"f_", ",", "l_"}], "]"}], ":=", 
   RowBox[{"Rest", "[", 
    RowBox[{"FoldList", "[", 
     RowBox[{"f", ",", "0", ",", "l"}], "]"}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"PolyNeg", "[", 
    RowBox[{"P_", ",", 
     RowBox[{"{", 
      RowBox[{"x_", ",", "y_"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"dx_", ",", "dy_"}], "}"}]}], "]"}], ":=", 
   RowBox[{"(", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{"Fold", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"CumFun", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"Min", "[", 
                RowBox[{"#1", ",", "0"}], "]"}], "/", "dy"}], "+", "#2"}], 
             "&"}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"Map", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"Max", "[", 
                  RowBox[{"#1", ",", "0"}], "]"}], "&"}], ",", "#1"}], "]"}], 
              "dx"}], "+", "#2"}]}], "]"}], "&"}], ",", "0", ",", 
         RowBox[{"Reverse", "[", 
          RowBox[{"CoefficientList", "[", 
           RowBox[{"P", ",", 
            RowBox[{"{", 
             RowBox[{"x", ",", "y"}], "}"}]}], "]"}], "]"}]}], "]"}], "//", 
       "Max"}], ")"}], "\[LessEqual]", "0"}], ")"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.548980367949332*^9, 3.548980384326022*^9}, {
  3.549046744450163*^9, 3.5490467707514973`*^9}, {3.549046825680255*^9, 
  3.549046827752047*^9}}]
}, Open  ]],

